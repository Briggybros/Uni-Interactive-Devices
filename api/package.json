{
  "name": "api",
  "version": "1.0.0",
  "description": "",
  "main": "server/dist/index.js",
  "scripts": {
    "start": "node server/dist/index.js",
    "build": "run-s build:*",
    "build:server": "rimraf server/dist && babel server/src -d server/dist --extensions .ts --copy-files",
    "build:frontend": "rimraf frontend/dist && ENV=development webpack -d --config ./frontend/webpack.config.js",
    "test": "npm run reformat && run-p test:*",
    "test:tsc": "tsc --noEmit",
    "test:tslint": "tslint --project .",
    "reformat": "run-p reformat:*",
    "reformat:server": "prettier --write 'server/src/**/*@(.js|.ts|.jsx|.tsx|.css)'",
    "reformat:frontend": "prettier --write 'frontendapp/src/**/*@(.js|.ts|.jsx|.tsx|.css)'"
  },
  "author": "Gregory Sims",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.1.5",
    "@babel/core": "^7.1.6",
    "@babel/preset-env": "^7.1.6",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.1.0",
    "@types/body-parser": "^1.17.0",
    "@types/ejs": "^2.6.0",
    "@types/express": "^4.16.0",
    "@types/react": "^16.7.7",
    "@types/react-dom": "^16.0.10",
    "@types/react-router": "^4.4.1",
    "@types/react-router-dom": "^4.3.1",
    "@types/uuid": "^3.4.4",
    "babel-loader": "^8.0.0-beta.6",
    "clean-webpack-plugin": "^1.0.0",
    "css-loader": "^1.0.1",
    "html-webpack-plugin": "^3.2.0",
    "npm-run-all": "^4.1.3",
    "prettier": "^1.15.2",
    "rimraf": "^2.6.2",
    "style-loader": "^0.23.1",
    "tslint": "^5.11.0",
    "tslint-config-airbnb": "^5.11.1",
    "tslint-config-prettier": "^1.16.0",
    "typescript": "^3.1.6",
    "webpack": "^4.26.0",
    "webpack-cli": "^3.1.2"
  },
  "dependencies": {
    "express": "^4.16.4",
    "react": "^16.7.0-alpha.2",
    "react-dom": "^16.7.0-alpha.2",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "uuid": "^3.3.2"
  }
}
